{
  "name": "恶意网址检测",
  "version": "2.1",
  "permissions": [
    "tabs",
    "checkmurl",
    "activeTab", 
    "http://*/*", 
    "https://*/*", 
    "contextMenus", 
    "webNavigation", 
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "key" : "",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
